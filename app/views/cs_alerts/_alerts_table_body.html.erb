<tbody>
<% @alerts.each do |alert| %>
  <tr>
    <td>
      <%= alert.student.name %>
    </td>
    <td>
      <%= link_to alert.assignment.name, alert.assignment_id.to_s %>
    </td>
    <td>
      <%= link_to("Add more attempts") %>
    </td>
    <td>
      <%= form_tag "/cs_alerts/#{alert.alert_id}", method: 'delete' do %>    
        <%= hidden_field_tag :alert_id, alert.alert_id %> 
        <%= submit_tag 'dismiss' %>
      <% end %>
    </td>
  </tr>
<% end %>
</tbody>